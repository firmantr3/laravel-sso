{
    "name": "firmantr3/laravel-sso",
    "description": "Laravel Single Sign On (SSO) authenticatable credentials implementation.",
    "type": "library",
    "keywords": [
        "laravel",
        "sso",
        "single sign on",
        "provider",
        "user"
    ],
    "require": {
        "illuminate/auth": "^5.0|^6.0",
        "illuminate/container": "^5.0|^6.0",
        "illuminate/contracts": "^5.0|^6.0",
        "illuminate/database": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Firman Taruna Nugraha",
            "email": "firmantr3@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Firmantr3\\LaravelSSO\\": "src/Firmantr3/LaravelSSO/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firmantr3\\LaravelSSO\\Test\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firmantr3\\LaravelSSO\\Providers\\SSOServiceProvider"
            ]
        }
    }
}
